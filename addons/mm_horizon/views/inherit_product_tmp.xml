<odoo>
    <data>
        <record id="view_inherit_product_template_mm" model="ir.ui.view">
            <field name="name">view.inherit.product.template.mm</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='uom_po_id']" position="after">
                    <field name="mm_factor"/>
                    <field name="mm_uom"/>
                    <field name="mm_term"/>
                </xpath>
             </field>
        </record>
    </data>
</odoo>