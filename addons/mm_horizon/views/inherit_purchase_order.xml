<odoo>
    <data>
        <record id="view_inherit_purchase_order_mm" model="ir.ui.view">
            <field name="name">view.inherit.purchase.order.mm</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='products']/field[@name='order_line']/tree[1]/field[@name='product_qty']"
                       position="after">
                    <field name="mm_factor_l"/>
                    <field name="mm_uom_l"/>
                    <field name="mm_fac_can"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>