<odoo>
    <data>
        <record id="view_inherit_sale_order_mm" model="ir.ui.view">
            <field name="name">view.inherit.sale.order.mm</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='payment_term_id']" position="after">
                    <field name="fechapro" widget="datetime" readonly="0"/>
                    <field name="camp"/>
                </xpath>
                <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='order_lines']/field[@name='order_line']/tree[1]/field[@name='sequence']" position="after">
                    <field name="fechalst" widget="datetime" readonly="0"/>
                </xpath>
             </field>
        </record>
    </data>
</odoo>