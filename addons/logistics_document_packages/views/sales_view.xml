<odoo>
    <data>
        <record id="view_order_form_inherit" model="ir.ui.view">
            <field name="name">sale.order.form.inherit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='customer_signature']" position="after">
                    <page string="Logistica" name="customs">
                        <group>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
                            <field name="fletero" placeholder="Ej: Transportista López" readonly="0"/>
                            <field name="downloaded_attachment_ids"  widget="many2many" readonly="0"/>
                            <field name="proof_of_delivery" widget="many2many_binary" readonly="0"/>
                            <field name="partner_address_full" invisible="1"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
        <record id="custom_sale_order" model="ir.ui.view">
            <field name="name">sale.order.form.inherit.logistics</field>
            <field name="model">sale.order</field>
            <field name="arch" type="xml">
                <form>
                    <group string="Datos de Logistica">
                        <field name="fletero" placeholder="Ej: Transportista López" readonly="0"/>
                        <field name="downloaded_attachment_ids" widget="many2many_binary"  readonly="1"/>
                        <field name="proof_of_delivery" widget="many2many_binary" readonly="0"/>
                    </group>
                </form>
            </field>
        </record>
    </data>
</odoo>