<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="model_hs7_form">
            <field name="name">model hs7</field>
            <field name="model">model.hs7</field>
            <field name="arch" type="xml">
              <form>
                 <sheet>
                  <group>
                    <field name="name">model </field>
                    <field name="ref_trailer"/>
                    <field name="model"/>
                  </group>
                </sheet>
              </form>
            </field>  
        </record> 
        <record model="ir.ui.view" id="freight_list">
          <field name="name">model hs7 list</field>
          <field name="model">model.hs7</field>
          <field name="arch" type="xml">
            <tree create="false">
                <field name="name"></field>
                <field name="ref_trailer"/>
                <field name="model"/>
            </tree>
          </field>
        </record>
        
        <record model="ir.actions.act_window" id="model_hs7_form_action_window">
            <field name="name">MODEL HS7</field>
            <field name="res_model">model.hs7</field>
            <field name="view_mode">form</field>
        </record>

        <record model="ir.actions.act_window" id="model_log_action_window">
          <field name="name">MODEL HS7 LOG</field>
          <field name="res_model">model.hs7</field>
          <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Model HS7 Options" id="options_model" parent="logistics_document_packages_menu_root"/>

        <menuitem name="Add Model HS7" id="new_model" parent="options_model" action="model_hs7_form_action_window"/>

        <menuitem name="Model HS7 registry" id="model_registry" parent="options_model" action="model_log_action_window"/>
    </data>
</odoo>